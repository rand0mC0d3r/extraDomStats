{
  "manifest_version": 3,
  "name": "test test",
  "version": "1.0.1",
  "description": "test test",
  "action": { "default_popup": "index.html" },
  "permissions": ["activeTab", "storage", "sidePanel", "nativeMessaging"],
  "content_scripts": [
    {
      "js": ["src/Stats.jsx"],
